10. Zahlensysteme
import java.util.Scanner;

public class ZahlenUmrechner {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        boolean weiter = true;

        while (weiter) {
            System.out.print("Men√º: Zeichen, Oktal, heX, Ende? ");
            String auswahl = scanner.nextLine().toUpperCase();

            switch (auswahl) {
                case "Z":
                    System.out.print("Bitte gib ein Zeichen ein: ");
                    char zeichen = scanner.nextLine().charAt(0);
                    int ascii = (int) zeichen;
                    System.out.println("Dezimal: " + ascii);
                    System.out.println("Bin√§r: " + Integer.toBinaryString(ascii));
                    break;

                case "O":
                    System.out.print("Bitte gib eine Oktalzahl ein: ");
                    String oktal = scanner.nextLine();
                    try {
                        int dezOktal = Integer.parseInt(oktal, 8);
                        System.out.println("Dezimal: " + dezOktal);
                        System.out.println("Bin√§r: " + Integer.toBinaryString(dezOktal));
                    } catch (NumberFormatException e) {
                        System.out.println("Ung√ºltige Oktalzahl!");
                    }
                    break;

                case "X":
                    System.out.print("Bitte gib eine Hexadezimalzahl ein: ");
                    String hex = scanner.nextLine();
                    try {
                        int dezHex = Integer.parseInt(hex, 16);
                        System.out.println("Dezimal: " + dezHex);
                        System.out.println("Bin√§r: " + Integer.toBinaryString(dezHex));
                    } catch (NumberFormatException e) {
                        System.out.println("Ung√ºltige Hexadezimalzahl!");
                    }
                    break;

                case "E":
                    weiter = false;
                    System.out.println("Programm beendet.");
                    break;

                default:
                    System.out.println("Ung√ºltige Auswahl.");
            }
        }

        scanner.close();
    }
}
