4. Fakult√§t berechnen (lassen)
import java.util.Scanner;

public class fakult√§tRechner {

    public static long berechnenFakult√§t(int n) {
        long ergebnis = 1;
        for (int i = 1; i <= n; i++) {
            ergebnis *= i;
        }
        return ergebnis;
    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Bitte gib eine Zahl ein: ");
        int zahl = scanner.nextInt();

        if (zahl < 0) {
            System.out.println("Fakult√§t ist nur f√ºr nicht-negative Zahlen definiert.");
        } else {
            long Fakult√§t = berechnenFakult√§t(zahl);
            System.out.println("Die Fakult√§t von " + zahl + " ist: " + Fakult√§t);
        }

        scanner.close();
    }
}
